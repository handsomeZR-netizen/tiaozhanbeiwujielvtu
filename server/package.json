{
  "name": "boundless-lens-server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "prebuild": "node copy-demo-images.js",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:unit": "vitest --run src/tests/itineraries.api.test.ts",
    "test:properties": "vitest --run src/tests/itineraries.properties.test.ts",
    "test:smoke": "tsx src/tests/smoke.ts",
    "test:itinerary": "tsx src/tests/itineraryGenerator.test.ts",
    "test:oss": "tsx test-oss-complete.ts",
    "test:env": "tsx check-railway-env.ts",
    "test:all": "bash test-all.sh",
    "db:init": "tsx scripts/init-db.ts",
    "db:check": "tsx scripts/check-db-tables.ts",
    "db:export": "tsx scripts/export-local-data.ts",
    "db:import": "tsx scripts/import-data.ts",
    "generate:community-images": "tsx scripts/generate-community-images.ts",
    "test:watermark": "tsx scripts/test-watermark.ts",
    "compress:images": "tsx scripts/compress-images.ts"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/static": "^9.0.0",
    "ali-oss": "^6.23.0",
    "dotenv": "^16.6.1",
    "fastify": "^5.2.0",
    "pg": "^8.12.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "fast-check": "^4.5.3",
    "sharp": "^0.34.5",
    "tsx": "^4.19.2",
    "typescript": "~5.8.2",
    "vitest": "^4.0.18"
  }
}
